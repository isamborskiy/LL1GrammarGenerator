grammar Postfix;

s[int i] returns [String val]	
	| e EOF
	| EPS EOF
	;

e
	| t es
	;
	
es
	| OR t es
	| XOR t es
	| EPS
	;
	
t
	| f ts
	;
	
ts	
	| AND f ts
	| EPS
	;
	
f
	| NOT f
	| V
	| LEFT e RIGHT
	;
	
	
OR		: (or);
XOR		: (xor);
AND		: (and);
NOT		: (not);
LEFT	: ([\(]);
RIGHT	: ([\)]);
V		: ([a-z]+);
SKIP	: ([ \t\r\n]) -> skip;