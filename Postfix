grammar Postfix;

s[int i] returns [String val]	
	| e[i] EOF
	| EPS EOF
	;

e[int i]
	| t[i] es[i]
	;
	
es[int i]
	| OR t[i] es[i]
	| XOR t[i] es[i]
	| EPS
	;
	
t[int i]
	| f[i] ts[i]
	;
	
ts[int i]
	| AND f[i] ts[i]
	| EPS
	;
	
f[int i]
	| NOT f[i]
	| V
	| LEFT e[i] RIGHT
	;
	
	
OR		: (or);
XOR		: (xor);
AND		: (and);
NOT		: (not);
LEFT	: ([\(]);
RIGHT	: ([\)]);
V		: ([a-z]+);
SKIP	: ([ \t\r\n]) -> skip;