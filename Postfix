grammar Postfix $

s[int i]
	# N a EOF
	{System.out.println(_0.text + _1.val);}
	# EPS EOF
	{System.out.println("EMPTY INPUT");}
	$
	
a returns [String val]
	# N a SIGN a
	{val = _0.text + _1.val + _2.text + _3.val;}
	# EPS
	{val = "";}
	$
	
SIGN: ([+\-*]) $
N	: (0|([1-9][0-9]*)) $
SKIP: ([ \t\r\n]) -> skip $